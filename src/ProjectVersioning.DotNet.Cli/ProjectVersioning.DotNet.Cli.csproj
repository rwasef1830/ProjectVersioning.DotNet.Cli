<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Description>.NET Core CLI project version generator tool</Description>
    <Authors>Raif Atef</Authors>
    <TargetFramework>netcoreapp1.0</TargetFramework>
    <AssemblyName>dotnet-project-version</AssemblyName>
    <OutputType>Exe</OutputType>
    <PackageId>ProjectVersioning.DotNet.Cli</PackageId>
    <Version>2.1.3</Version>
    <PackageLicenseUrl>https://opensource.org/licenses/MIT</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/rwasef1830/ProjectVersioning.DotNet.Cli</PackageProjectUrl>
    <RepositoryUrl>https://github.com/rwasef1830/ProjectVersioning.DotNet.Cli</RepositoryUrl>
    <PackageReleaseNotes />
    <PackageTags>.net dotnet-cli versioning hg git</PackageTags>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <AssemblyVersion>2.1.3.0</AssemblyVersion>
    <FileVersion>2.1.3.0</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.ComponentModel.TypeConverter" Version="4.3.0" />
  </ItemGroup>

</Project>
